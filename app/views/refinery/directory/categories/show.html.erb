<% content_for :body_content_left do %>
  <ul id="contacts">
    <%contacts = @category.contacts%>
    <% contacts.each do |contact| %>
      <li>
        <%= link_to contact.name, refinery.directory_contact_path(contact) %>
      <br>
        <%= contact.abstract%>
      </li>
    <% end %>
  </ul>
<% end %>

<%= render '/refinery/content_page' %>
